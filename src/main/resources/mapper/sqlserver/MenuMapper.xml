        <result column="modify_time" property="modifyTime" />
        <result column="del_flag" property="delFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, parent_id, name, icon, sort_index, url, create_time, modify_time, del_flag
    </sql>

</mapper>
